{
	"manifest_version": 2,
	"name": "Chrome插件名字",
	"version": "1.0",
	"description": "Chrome插件描述",
	"author": "Chrome插件作者名",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["js/content-script.js"]
		}
	]
	// 详见http://open.chrome.360.cn/extension_dev/overview.html
}